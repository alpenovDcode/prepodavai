  },
  "dependencies": {
    "grammy": "^1.20.0",
    "dotenv": "^16.3.1",
    "@prisma/client": "^5.10.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "prisma": "^5.10.0"
  }
}