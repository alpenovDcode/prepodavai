{
  "name": "@prepodavai/telegram-bot",
  "version": "1.0.0",
  "description": "Telegram бот для PrepodavAI",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "start:dev": "ts-node-dev --respawn src/main.ts",
